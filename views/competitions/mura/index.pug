extends ../../layout

block title
    title MURA Dataset: Towards Radiologist-Level Abnormality Detection in Musculoskeletal Radiographs

block description
  meta(name='description', content='We introduce MURA, a large dataset of musculoskeletal radiographs containing 40,895 images from 14,982 studies, where each study is manually labeled by radiologists as either normal or abnormal.')

block extralinks
  link(rel='stylesheet', type='text/css', href='/projects/mura/css/index.css')

block extrascripts
